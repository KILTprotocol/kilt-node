{
  "name": "@kiltprotocol/zombienet-networks",
  "private": "true",
  "license": "MIT",
  "devDependencies": {
    "@zombienet/cli": "^1.3.109"
  },
  "scripts": {
    "spawn:peregrine": "yarn zombienet spawn --spawn-concurrency=1 runtimes/peregrine/network.yaml",
    "span:peregrine:with-env": "env $(cat runtimes/peregrine/.env | xargs) yarn spawn:peregrine",
    "spawn:spiritnet": "yarn zombienet spawn runtimes/spiritnet/network.yaml",
    "spawn:spiritnet:with-env": "env $(cat runtimes/spiritnet/.env | xargs) yarn spawn:spiritnet"
  }
}
